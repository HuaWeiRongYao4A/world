<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.world.producer.mapper.UserMapper">

    <resultMap id="baseResultMap" type="com.world.producer.entity.User">
        <id column="id" property="id" jdbcType="DECIMAL" />
        <result column="username" property="username" jdbcType="VARCHAR" />
        <result column="password" property="password" jdbcType="VARCHAR" />
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
        <result column="update_time" property="updateTime" jdbcType="TIMESTAMP" />
        <result column="email" property="email" jdbcType="VARCHAR" />
        <result column="active_code" property="activeCode" jdbcType="TIMESTAMP" />
        <result column="is_active" property="isActive" jdbcType="DECIMAL" />
    </resultMap>

    <insert id="insert" parameterType="com.world.producer.entity.User">
        INSERT INTO user_info (
        username,
        password,
        create_time,
        update_time,
        email,
        active_code,
        is_active
        ) VALUES (
        #{username, jdbcType=VARCHAR},
        #{password, jdbcType=VARCHAR},
        sysdate(),
        sysdate(),
        #{email, jdbcType=VARCHAR},
        #{activeCode, jdbcType=VARCHAR},
        #{isActive, jdbcType=DECIMAL}
        )
    </insert>

</mapper>